XTerm*internalBorder: 10
XTerm*metaSendsEscape: true
XTerm*eightBitInput: false
XTerm*faceName: Input Mono Condensed Regular
! XTerm*faceName: DejaVu Sans Mono
XTerm*faceSize: 14


! Схема tangoesque
xterm*background: #111111
xterm*foreground: #babdb6
! Black (not tango) + DarkGrey (Чёрный (не танго)+ТёмноСерый)
xterm*color0:  #000000
xterm*color8:  #555753
! DarkRed + Red (ТёмноКрасный+Красный)
xterm*color1:  #ff6565
xterm*color9:  #ff8d8d
! DarkGreen + Green (ТёмноЗелёный+Зелёный)
xterm*color2:  #93d44f
xterm*color10: #c8e7a8
! DarkYellow + Yellow (ТёмноЖёлтый+Жёлтый)
xterm*color3:  #eab93d
xterm*color11: #ffc123
! DarkBlue + Blue (ТёмноСиний+Синий)
xterm*color4:  #204a87
xterm*color12: #3465a4
! DarkMagenta + Magenta (ТёмноПурпурный+Пурпурный)
xterm*color5:  #ce5c00
xterm*color13: #f57900
!DarkCyan + Cyan (both not tango) (ТёмноГолубой+Голубой (оба не танго))
xterm*color6:  #89b6e2
xterm*color14: #46a4ff
! LightGrey + White (СветлоСерый+Белый)
xterm*color7:  #cccccc
xterm*color15: #ffffff

